{
  "name": "express-svelte-template",
  "version": "1.0.0",
  "description": "Template for building Express, Svelte Webapps",
  "main": "src/main.js",
  "scripts": {
    "build:svelte": "rollup -c",
    "dev:svelte": "rollup -c -w",
    "start:svelte": "sirv public",
    "dev:node": "nodemon src/main.js",
    "dev": "run-p dev:node dev:svelte"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "sirv-cli": "^1.0.0"
  }
}
